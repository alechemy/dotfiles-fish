// Many of these settings are inspired by https://github.com/antfu/vscode-settings
{
  // =========================================
  // GENERAL
  // =========================================

  // EDITOR
  // ----------------------------------------
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.colorDecorators": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.find.addExtraSpaceOnTop": false,
  "editor.find.seedSearchStringFromSelection": "never",
  "editor.fontFamily": "Operator Mono SSm Lig",
  "editor.fontLigatures": true,
  // "editor.fontFamily": "'Monaspace Neon', monospace",
  // "editor.fontLigatures": "'calt', 'liga', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'ss08', 'ss09'",
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.addMissingImports": "explicit",
    "source.removeUnusedImports": "explicit",
    "source.fixAll.eslint": "explicit"
  },
  "editor.guides.bracketPairs": "active",
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "editor.pasteAs.preferences": ["text.updateImports"],
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [
    {
      "column": 80,
      "color": "#102a44"
    }
  ],
  "editor.snippetSuggestions": "bottom",
  "editor.stickyScroll.enabled": true,
  "editor.suggest.localityBonus": true,
  "editor.suggestSelection": "first",
  "editor.wordWrap": "on",
  // Prefer direct navigation over the "peek" mini-window
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "editor.semanticTokenColorCustomizations": {
    "rules": {
      "property.deprecated": { "strikethrough": false },
      "method.deprecated": { "strikethrough": false },
      "function.deprecated": { "strikethrough": false }
    }
  },

  // FILES
  // ----------------------------------------
  "files.exclude": {
    "**/.cache": true,
    "**/node_modules": true,
    "**/target": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "files.simpleDialog.enable": true,
  // "files.insertFinalNewline": true,
  // "files.trimFinalNewlines": true,
  // "files.trimTrailingWhitespace": true,

  // PRIVACY & TELEMETRY
  // ----------------------------------------
  "telemetry.telemetryLevel": "off",
  "redhat.telemetry.enabled": false,
  "gitlens.telemetry.enabled": false,

  // SEARCH
  // ----------------------------------------
  "search.exclude": {
    "**/.angular": true,
    "**/.git": true,
    "**/.nuxt": true,
    "**/.nx": true,
    "**/.output": true,
    "**/.pnpm": true,
    "**/.vscode": true,
    "**/.venv": true,
    "**/.yarn": true,
    "**/bower_components": true,
    "**/coverage": true,
    "**/dist/**": true,
    "**/logs": true,
    "**/node_modules": true,
    "**/out/**": true,
    "**/package-lock.json": true,
    "**/pnpm-lock.yaml": true,
    "**/tmp": true,
    "**/yarn.lock": true
  },
  "search.quickOpen.history.filterSortOrder": "recency",
  "search.quickOpen.includeHistory": false,
  "search.useIgnoreFiles": false,

  // SECURITY
  // ----------------------------------------
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.startupPrompt": "never",

  // WINDOW
  // ----------------------------------------
  "window.title": "${rootName}", // this make tabs more readable
  "window.titleBarStyle": "custom",

  // WORKBENCH
  // ----------------------------------------
  "workbench.colorCustomizations": {
    "[Night Owl]": {
      "editor.wordHighlightBackground": "#FFFFFF08"
    }
  },
  "workbench.colorTheme": "Night Owl",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.alwaysShowEditorActions": true,
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.limit.excludeDirty": true,
  "workbench.editor.limit.value": 5,
  "workbench.enableExperiments": false,
  "workbench.fontAliasing": "antialiased",
  "workbench.iconTheme": "vscode-icons",
  "workbench.settings.editor": "json",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "newUntitledFile",

  // =========================================
  // LANGUAGES & TOOLS
  // =========================================

  // FISH
  // ----------------------------------------
  "[fish]": {
    "editor.defaultFormatter": "ndonfris.fish-lsp"
  },
  "fish.path.fish": "/opt/homebrew/bin/fish",
  "fish.path.fish_indent": "/opt/homebrew/bin/fish_indent",

  // GIT / GITHUB
  // ----------------------------------------
  "git.autofetch": false,
  "githubPullRequests.pullBranch": "never",
  "gitlens.remotes": [
    {
      "domain": "git.digitalsmiths.net",
      "type": "GitLab"
    }
  ],

  // HTML
  // ----------------------------------------
  "[html]": {
    "editor.rulers": [
      {
        "column": 100,
        "color": "#102a44"
      }
    ]
  },

  // JAVA
  // ----------------------------------------
  "java.jdt.ls.java.home": "/Library/Java/JavaVirtualMachines/openjdk-21.jdk/Contents/Home",
  "java.configuration.runtimes": [
    {
      "default": true,
      "name": "JavaSE-21",
      "path": "/Library/Java/JavaVirtualMachines/openjdk-21.jdk/Contents/Home"
    }
  ],
  "java.project.explorer.showNonJavaResources": false,

  // JAVASCRIPT
  // ----------------------------------------
  "javascript.preferences.importModuleSpecifier": "relative",
  "javascript.updateImportsOnFileMove.enabled": "never",

  // PYTHON
  // ----------------------------------------
  "python.defaultInterpreterPath": "/opt/homebrew/bin/python3",
  "python.languageServer": "Pylance",
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff"
  },

  // SHELLSCRIPT
  // ----------------------------------------
  "[shellscript]": {
    "editor.defaultFormatter": "mkhl.shfmt"
  },

  // TERMINAL
  // ----------------------------------------
  "terminal.integrated.fontFamily": "Operator Mono SSm Lig",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.profiles.osx": {
    "fish (homebrew)": {
      "path": "/opt/homebrew/bin/fish"
    }
  },
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.defaultProfile.osx": "fish (homebrew)",

  // TERRAFORM
  // ----------------------------------------
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform"
  },

  // TYPESCRIPT
  // ----------------------------------------
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.updateImportsOnFileMove.enabled": "never",

  // XML
  // ----------------------------------------
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },

  // INLINE FOLD
  // ----------------------------------------
  "inlineFold.maskChar": "â€¦",
  "inlineFold.maskCss": "color: #808080",
  "inlineFold.regex": "(class|className)=[`'{\"]([^`'\"}]{50,})[`'\"}]",
  "inlineFold.regexFlags": "g",
  "inlineFold.regexGroup": "2",
  "inlineFold.supportedLanguages": [
    "astro",
    "vue",
    "html",
    "svelte",
    "vue-html",
    "php",
    "blade",
    "erb",
    "twig",
    "nunjucks",
    "django-html",
    "jinja-html",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "markdown"
  ],
  "inlineFold.unfoldOnLineSelect": true,
  "inlineFold.unfoldedOpacity": 1,
  "[markdown]": {
    "inlineFold.regex": "\\[.*\\]\\((.*)\\)",
    "inlineFold.regexFlags": "g",
    "inlineFold.regexGroup": "1"
  },
  "[svg]": {
    "inlineFold.regex": "<svg(\\s*.*?\\s*)<\\/svg>",
    "inlineFold.regexFlags": "gs",
    "inlineFold.regexGroup": "2"
  },

  // =========================================
  // EVERYTHING ELSE
  // =========================================
  "chat.commandCenter.enabled": false,
  "colorize.enable_search_variables": false,
  "colorize.languages": [
    "css",
    "sass",
    "scss",
    "less",
    "postcss",
    "sss",
    "stylus",
    "xml",
    "svg",
    "js",
    "ts"
  ],
  "colorize.include": [
    "**/*.css",
    "**/*.scss",
    "**/*.sass",
    "**/*.less",
    "**/*.styl",
    "**/*.js",
    "**/*.ts"
  ],
  "cSpell.allowCompoundWords": true,
  "cSpell.language": "en,en-US",
  "cSpell.showInRuler": false,
  "cSpell.userWords": ["Gifs", "Karabiner", "tivo", "uirouter"],
  "[csv]": {
    "editor.inlayHints.maximumLength": 0
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "errorLens.enabledDiagnosticLevels": ["error"],
  "errorLens.excludeBySource": ["cSpell", "eslint"],
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "explorer.confirmDragAndDrop": false,
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "fileNestingUpdater.autoUpdateInterval": 10080, // 1 week
  "fileNestingUpdater.promptOnAutoUpdate": false,
  "maven.executable.path": "/opt/homebrew/bin/mvn",
  "peacock.affectActivityBar": false,
  "peacock.affectStatusBar": false,
  "peacock.showColorInStatusBar": false,
  "projectManager.git.baseFolders": ["~/Work"],
  "projectManager.tags": ["1_Work", "2_Personal", "3_Roles"],
  "rainbow_csv.virtual_alignment_mode": "always",
  "shfmt.executablePath": "/opt/homebrew/bin/shfmt",
  "terraform.languageServer.ignoreSingleFileWarning": true,
  "vsicons.dontShowNewVersionMessage": true,
  "multiCommand.commands": [
    {
      "command": "multiCommand.toggleQuietRead",
      "sequence": [
        { "command": "errorLens.toggleError" },
        { "command": "disable-error-squiggles.toggleErrorWarningColors" }
      ]
    }
  ],

