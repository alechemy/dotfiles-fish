// Many of these settings are inspired by https://github.com/antfu/vscode-settings
{
  // =========================================
  // GENERAL
  // =========================================

  // EDITOR
  // ----------------------------------------
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.colorDecorators": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.find.addExtraSpaceOnTop": false,
  "editor.fontFamily": "Operator Mono SSm Lig",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "editor.minimap.renderCharacters": false,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [
    {
      "column": 80,
      "color": "#102a44"
    }
  ],
  "editor.snippetSuggestions": "bottom",
  "editor.stickyScroll.enabled": true,
  "editor.suggest.localityBonus": true,
  "editor.suggestSelection": "first",
  // Prefer direct navigation over the "peek" mini-window
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",

  // FILES
  // ----------------------------------------
  "files.exclude": {
    "**/.cache": true,
    "**/node_modules": true,
    "**/target": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "files.simpleDialog.enable": true,
  // "files.insertFinalNewline": true,
  // "files.trimFinalNewlines": true,
  // "files.trimTrailingWhitespace": true,

  // PRIVACY & TELEMETRY
  // ----------------------------------------
  "redhat.telemetry.enabled": false,
  "telemetry.telemetryLevel": "off",

  // SEARCH
  // ----------------------------------------
  "search.exclude": {
    "**/.git": true,
    "**/.nuxt": true,
    "**/.output": true,
    "**/.pnpm": true,
    "**/.vscode": true,
    "**/.yarn": true,
    "**/bower_components": true,
    "**/dist/**": true,
    "**/logs": true,
    "**/node_modules": true,
    "**/out/**": true,
    "**/package-lock.json": true,
    "**/pnpm-lock.yaml": true,
    "**/tmp": true,
    "**/yarn.lock": true
  },
  "search.quickOpen.history.filterSortOrder": "recency",
  "search.useIgnoreFiles": false,

  // SECURITY
  // ----------------------------------------
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.startupPrompt": "never",

  // STYLING
  // ----------------------------------------
  "customizeUI.activityBar": "wide",
  "customizeUI.titleBar": "inline",
  "customizeUI.stylesheet": {
    // Boost the width of the Quick Open panel (useful for viewing very/long/file/paths)
    // Upstream issue: https://github.com/microsoft/vscode/issues/85374
    ".quick-input-widget": "width: 60% !important; left: 40%"
  },

  // WINDOW
  // ----------------------------------------
  "window.titleBarStyle": "custom",

  // WORKBENCH
  // ----------------------------------------
  "workbench.colorCustomizations": {
    "[Night Owl]": {
      "activityBar.background": "#000C1D",
      "activityBar.border": "#102a44",
      "sideBar.background": "#001122",
      "sideBar.border": "#102a44",
      "sideBar.foreground": "#8BADC1"
    },
    "[Night Owl No Italics]": {
      "activityBar.background": "#000C1D",
      "activityBar.border": "#102a44",
      "sideBar.background": "#001122",
      "sideBar.border": "#102a44",
      "sideBar.foreground": "#8BADC1"
    }
  },
  "workbench.colorTheme": "Night Owl",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.limit.value": 5,
  "workbench.enableExperiments": false,
  "workbench.fontAliasing": "antialiased",
  "workbench.iconTheme": "vscode-icons",
  "workbench.settings.editor": "json",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.startupEditor": "newUntitledFile",

  // =========================================
  // LANGUAGES & TOOLS
  // =========================================

  // FISH
  // ----------------------------------------
  "[fish]": {
    "editor.defaultFormatter": "bmalehorn.vscode-fish"
  },
  "fish.path.fish": "/opt/homebrew/bin/fish",
  "fish.path.fish_indent": "/opt/homebrew/bin/fish_indent",

  // GIT / GITHUB
  // ----------------------------------------
  "git.autofetch": false,
  "githubPullRequests.pullBranch": "never",

  // JAVASCRIPT
  // ----------------------------------------
  "javascript.preferences.importModuleSpecifier": "relative",
  "javascript.updateImportsOnFileMove.enabled": "never",
  // allow Path Intellisense to act as default autocompletion engine
  "javascript.suggest.paths": false,

  // PYTHON
  // ----------------------------------------
  "python.defaultInterpreterPath": "/opt/homebrew/bin/python3",
  "python.languageServer": "Pylance",

  // SHELLSCRIPT
  // ----------------------------------------
  "[shellscript]": {
    "editor.defaultFormatter": "mkhl.shfmt"
  },

  // TERMINAL
  // ----------------------------------------
  "terminal.integrated.fontFamily": "Operator Mono SSm Lig",
  "terminal.integrated.defaultProfile.osx": "fish",
  "terminal.integrated.cursorStyle": "line",

  // TERRAFORM
  // ----------------------------------------
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform"
  },

  // TYPESCRIPT
  // ----------------------------------------
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.updateImportsOnFileMove.enabled": "never",
  // allow Path Intellisense to act as default autocompletion engine
  "typescript.suggest.paths": false,

  // INLINE FOLD
  // ----------------------------------------
  "inlineFold.maskChar": "â€¦",
  "inlineFold.maskColor": "#808080",
  "inlineFold.regex": "(class|className)=[`'{\"]([^`'\"}]{50,})[`'\"}]",
  "inlineFold.regexFlags": "g",
  "inlineFold.regexGroup": "2",
  "inlineFold.supportedLanguages": [
    "astro",
    "vue",
    "html",
    "svelte",
    "vue-html",
    "php",
    "blade",
    "erb",
    "twig",
    "nunjucks",
    "django-html",
    "jinja-html",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "markdown"
  ],
  "inlineFold.unfoldOnLineSelect": true,
  "inlineFold.unfoldedOpacity": 1,
  "[markdown]": {
    "inlineFold.regex": "\\[.*\\]\\((.*)\\)",
    "inlineFold.regexFlags": "g",
    "inlineFold.regexGroup": "1"
  },
  "[svg]": {
    "inlineFold.regex": "<svg(\\s*.*?\\s*)<\\/svg>",
    "inlineFold.regexFlags": "gs",
    "inlineFold.regexGroup": "2"
  },

  // =========================================
  // EVERYTHING ELSE
  // =========================================
  // editor.linkedEditing doesn't yet support JSX: https://github.com/microsoft/vscode/issues/85707
  "auto-rename-tag.activationOnLanguage": ["javascript", "javascriptreact"],
  "cSpell.allowCompoundWords": true,
  "cSpell.language": "en,en-US",
  "diffEditor.ignoreTrimWhitespace": false,
  "errorLens.enabledDiagnosticLevels": ["error"],
  "errorLens.excludeBySource": ["cSpell", "eslint"],
  "eslint.lintTask.enable": true,
  "eslint.format.enable": true,
  "explorer.confirmDragAndDrop": false,
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "fileNestingUpdater.autoUpdateInterval": 10080, // 1 week
  "fileNestingUpdater.promptOnAutoUpdate": false,
  "projectManager.git.baseFolders": [
    "/Users/alec/Work",
    "/Users/alec.custer/Work"
  ],
  "projectManager.tags": ["1_Work", "2_Personal", "3_Archive"],
  "shfmt.executablePath": "/opt/homebrew/bin/shfmt",
  "terraform.languageServer.ignoreSingleFileWarning": true,
  "vsicons.dontShowNewVersionMessage": true,

